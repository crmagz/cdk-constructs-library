{
    "name": "@cdk-constructs/cdk",
    "version": "0.1.0",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cristianmagana/cdk-constructs-library.git"
    },
    "workspaces": [
        "packages/*"
    ],
    "files": [
        "dist",
        "!packages"
    ],
    "scripts": {
        "clean": "rm -rf dist packages/*/dist && tsc --build tsconfig.build.json --clean",
        "lint": "eslint 'packages/*/src/**/*.ts' 'bin/**/*.ts' 'lib/**/*.ts' --max-warnings=0",
        "lint:fix": "eslint 'packages/*/src/**/*.ts' 'bin/**/*.ts' 'lib/**/*.ts' --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "format:fix": "npm run format && npm run lint:fix",
        "build:workspaces": "tsc --build tsconfig.build.json",
        "build:app": "tsc -p tsconfig.app.json",
        "build": "npm run build:workspaces && npm run build:app",
        "test": "echo \"No tests yet\" && exit 0",
        "synth": "cdk synth",
        "diff": "cdk diff",
        "deploy": "cdk deploy",
        "prepare": "husky",
        "postinstall": "rm -rf node_modules/@cdk-constructs/*/node_modules"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "20.11.30",
        "aws-cdk": "2.1032.0",
        "aws-cdk-lib": "2.225.0",
        "constructs": "10.4.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-n": "^17.17.0",
        "eslint-plugin-prettier": "^5.0.0",
        "gts": "^5.3.1",
        "husky": "^9.0.0",
        "jest": "^29.7.0",
        "lint-staged": "^15.0.0",
        "prettier": "^3.0.0",
        "ts-deepmerge": "^7.0.2",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.4.3"
    },
    "dependencies": {
        "@cdk-constructs/aurora": "*",
        "@cdk-constructs/aws": "*",
        "@cdk-constructs/cloudfront": "*",
        "@cdk-constructs/codeartifact": "*",
        "@cdk-constructs/route53": "*",
        "@cdk-constructs/s3": "*",
        "@cdk-constructs/waf": "*",
        "source-map-support": "^0.5.21"
    },
    "peerDependencies": {
        "aws-cdk-lib": "^2.225.0",
        "constructs": "^10.0.0"
    },
    "engines": {
        "node": ">=24"
    },
    "nodeAndNpmVersions": {
        "npmVersion": "10",
        "nodeVersion": "24"
    },
    "lint-staged": {
        "*.{ts,js,json,md}": [
            "prettier --write"
        ],
        "*.{ts,js}": [
            "eslint --fix"
        ]
    }
}
