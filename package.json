{
    "name": "@cdk-constructs/cdk",
    "version": "0.1.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cristianmagana/cdk-constructs-library.git"
    },
    "workspaces": [
        "packages/*"
    ],
    "files": [
        "dist",
        "!packages"
    ],
    "scripts": {
        "clean": "rm -rf dist packages/*/dist && tsc --build --clean",
        "lint": "eslint 'src/**/*.ts' 'packages/*/src/**/*.ts' --max-warnings=0",
        "lint:fix": "eslint 'src/**/*.ts' 'packages/*/src/**/*.ts' --fix",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "format:fix": "npm run format && npm run lint:fix",
        "build": "rm -rf dist && tsc -p tsconfig.json",
        "build:workspaces": "npm run build && cp -r dist node_modules/@cdk-constructs/cdk/ && tsc --build tsconfig.build.json",
        "prepare": "npm run build",
        "postinstall": "rm -rf node_modules/@cdk-constructs/*/node_modules"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "20.11.30",
        "aws-cdk": "2.1032.0",
        "aws-cdk-lib": "2.225.0",
        "constructs": "10.4.2",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-n": "^17.17.0",
        "eslint-plugin-prettier": "^5.0.0",
        "gts": "^5.3.1",
        "jest": "^29.7.0",
        "prettier": "^3.0.0",
        "ts-deepmerge": "^7.0.2",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "~5.4.3"
    },
    "dependencies": {
        "source-map-support": "^0.5.21"
    },
    "peerDependencies": {
        "aws-cdk-lib": "^2.225.0",
        "constructs": "^10.0.0"
    },
    "engines": {
        "node": ">=24"
    },
    "nodeAndNpmVersions": {
        "npmVersion": "10",
        "nodeVersion": "24"
    }
}
